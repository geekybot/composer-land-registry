<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Landria</title>
    <!-- Favicon-->
    <link rel="icon" href="../../img/logo.png" type="image/x-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    <!-- Bootstrap Core Css -->
    <link href="../../css/bootstrap.min.css" rel="stylesheet">

    <!-- Waves Effect Css -->
    <link href="../../css/waves.css" rel="stylesheet" />

    <!-- Animation Css -->
    <link href="../../css/animate.css" rel="stylesheet" />

    <!-- Custom Css -->
    <link href="../../css/style_admin.css" rel="stylesheet">

    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="../../css/all-themes.css" rel="stylesheet" />
</head>

<body class="theme-green">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Please wait...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
    <!-- Top Bar -->
    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="javascript:void(0);" class="bars"></a>
                <a class="navbar-brand anton" href="#">Landria</a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                </ul>
            </div>
        </div>
    </nav>
    <!-- #Top Bar -->
    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- User Info -->
            <div class="user-info">
                <div class="image">
                    <img src="../../img/user.png" width="48" height="48" alt="User" />
                </div>
                <div class="info-container">
                    <div class="name" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">John Doe</div>
                    <div class="email">john.doe@example.com</div>
                    <div class="btn-group user-helper-dropdown">
                        <i class="material-icons" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">keyboard_arrow_down</i>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="javascript:void(0);"><i class="material-icons">person</i>Profile</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="javascript:void(0);"><i class="material-icons">input</i>Sign Out</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                <ul class="list">
                    <li class="header">MAIN NAVIGATION</li>
                    <li class="active">
                        <a onClick="renderEjs('users/dashboard');">
                            <i class="material-icons">view_list</i>
                            <span>View Your Property</span>
                        </a>
                    </li>
                    <li>
                        <a onClick="renderEjs('users/property/register');">
                            <i class="material-icons">border_color</i>
                            <span>Register a Property</span>
                        </a>
                    </li>
                    <li>
                        <a onClick="renderEjs('users/property/sell');">
                            <i class="material-icons">attach_money</i>
                            <span>Sell a Property</span>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- #Menu -->
            <!-- Footer -->
            <div class="legal">
                <div class="copyright">
                    &copy; 2018 - 2019 <a href="javascript:void(0);">Landria</a>.
                </div>
                <div class="version">
                    <b>Version: </b> 1.0.5
                </div>
            </div>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->
    </section>

    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <div class="row clearfix">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="card">
                            <div class="header">
                                <h2>PROPERTY DETAILS</h2>
                            </div>
                            <div class="body">
                                <div class="row clearfix">
                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                        <p>
                                            <h2><span class="input-group-addon">
                                                    <i class="material-icons">person_pin</i>
                                                </span> PROPERTY ADDRESS: <span style="color: olivedrab;"><%=land.streetName,
                                                    land.locality %></span></h2>
                                        </p>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-lg-4 col-md-6 col-sm-12 ">
                                        <h2>OWNERSHIP HISTROY</h2>
                                        <div class="">
                                            <div class="ml-1">
                                                <h6><span style="color: greenyellow;font-size:1.2em;">• </span>Current
                                                    Owner</h6>
                                                <div class="ml-4">
                                                    <%= owner.fullName %>
                                                </div>
                                            </div>
                                            <!-- <div class="ml-3 mt-2">
                                                <h6><span style="color: grey;font-size:1.2em;">• </span>Previous Owner1</h6>
                                                <div class="ml-4">
                                                    Owner Name
                                                </div>
                                            </div>
                                            <div class="ml-4 mt-2">
                                                <h6><span style="color: grey;font-size:1.2em;">• </span>Previous Owner2</h6>
                                                <div class="ml-4">
                                                    Owner Name
                                                </div>
                                            </div> -->
                                        </div>
                                    </div>
                                    <div class="col-lg-8 col-md-6 col-sm-12">
                                        <div class="body table-responsive">
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th>PROPERTY NO.</th>
                                                        <th>ISSUE DATE</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td><%= land._id %></td>
                                                        <td><%= land.updatedAt.toISOString() %></td>
                                                    </tr>
                                                </tbody>
                                                <thead>
                                                    <tr>
                                                        <th>PLOT NO</th>
                                                        <th>STREET</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td><%= land.plotNumber %></td>
                                                        <td><%= land.streetName %></td>
                                                    </tr>
                                                </tbody>
                                                <thead>
                                                    <tr>
                                                        <th>CITY</th>
                                                        <th>STATE</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td><%= land.cityName %></td>
                                                        <td><%= land.state %></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                        <img src="<%= land.imageFileLink %>" class="card-img-top"
                                            alt="...">
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                        <img src="<%= land.deedFileLink %>" class="card-img-top"
                                            alt="...">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>

    <!-- Jquery Core Js -->
    <script src="../../js/jquery.min.js"></script>

    <!-- Jquery redirect plugin Js -->
    <script src="../../js/jquery.redirect.js"></script>

    <!-- Bootstrap Core Js -->
    <script src="../../js/bootstrap.js"></script>

    <!-- Select Plugin Js -->
    <script src="../../js/bootstrap-select.js"></script>

    <!-- Slimscroll Plugin Js -->
    <script src="../../js/jquery.slimscroll.js"></script>

    <!-- Waves Effect Plugin Js -->
    <script src="../../js/waves.js"></script>

    <!-- Custom Js -->
    <script src="../../js/admin.js"></script>
    <script src="../../js/index.js"></script>

    <!-- Demo Js -->
    <script src="../../js/demo.js"></script>
</body>

</html>